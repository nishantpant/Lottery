<div style="padding: 20px 80px;">
  <h1>Lottery Tickets</h1>
  <h4>Last Winner: </h4><%= @rss.summary %> 

  <%
  
    #11 - 35 - 38 - 41 - 52 MegaBall 40 Megaplier 4 
    winnerStr = @rss.summary.to_s
    winnerStr = winnerStr.gsub("Megaplier" , "-")
    winnerStr = winnerStr.gsub("MegaBall" , "-")
    winnerStr = winnerStr.gsub("\n" , "")
    arr = winnerStr.split(' - ')
  %>
     
  <div class="field">
    <table id="items" border="0">
      <tr>
        <th>Num 1
        <th>Num 2
        <th>Num 3
        <th>Num 4
        <th>Num 5
        <th>MegaPlier
      </tr>
      <% for ticket in @lotterytickets %>
      <tr>
        <td id="num1"><%= ticket.num1 %>
        <td id="num2"><%= ticket.num2 %>
        <td id="num3"><%= ticket.num3 %>
        <td id="num4"><%= ticket.num4 %>
        <td id="num5"><%= ticket.num5 %>
        <td id="megaplier"><%= ticket.megaplier %>
      </tr>
      <%  end %>
    </table>
  </div>
</div>
